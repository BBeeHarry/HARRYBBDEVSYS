        var formID = 'detailForm';
        var message = "";
        var form = $('#' + formID);
        $("#uploadItemTable").empty();
        $.validator.unobtrusive.parse(form);
        var validator = form.validate();
        if (form.valid()) {
            var url = '@Url.Action("SubmitMergeForm", "AdjustrefundUpload", new { Area = "Adjustrefund" })?' + form.serialize();
            $.ajax({
                url: url,
                type: 'post',
                //contentType: 'json',
                data: form.serialize(),
                success: function (response) {
                    unblockUI();
                    //$("#uploadItemBody").html(response.html);
                }
            }).done(function (result) {
                console.log("Hooray, it worked!");
                //submitFormDataIntegrateButton();
                //window.open(url);
                unblockUI();
            });
            //unblockUI();

            submitFormDataIntegrateButton();
            return false;
            @*$.post(form.attr("action"), form.serialize(), function (response) {

                if (response != null && response.success) {

                }
            });*@

        } else {

            for (var i = 0; i < validator.errorList.length; i++) {
                message = message + validator.errorList[i].message + "<br />";
                unblockUI();
            }
        }

    }
